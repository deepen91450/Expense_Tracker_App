<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deepenmandve.expensetrackerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.deepenmandve.expensetrackerapp.AccountActivity"></activity>
        <activity android:name="com.deepenmandve.expensetrackerapp.HistoryActivity" />
        <activity android:name="com.deepenmandve.expensetrackerapp.MonthlyAnalyticsActivity" />
        <activity android:name="com.deepenmandve.expensetrackerapp.WeeklyAnalyticsActivity" />
        <activity android:name="com.deepenmandve.expensetrackerapp.DailyAnalyticsActivity" />
        <activity android:name="com.deepenmandve.expensetrackerapp.ChooseAnalyticActivity" />
        <activity android:name="com.deepenmandve.expensetrackerapp.WeekSpendingActivity" />
        <activity android:name="com.deepenmandve.expensetrackerapp.TodaySpendingActivity" />
        <activity android:name="com.deepenmandve.expensetrackerapp.BudgetActivity" />
        <activity android:name="com.deepenmandve.expensetrackerapp.MainActivity" />
        <activity android:name="com.deepenmandve.expensetrackerapp.RegistrationActivity" />
        <activity android:name="com.deepenmandve.expensetrackerapp.LoginActivity" />
        <activity android:name="com.deepenmandve.expensetrackerapp.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>